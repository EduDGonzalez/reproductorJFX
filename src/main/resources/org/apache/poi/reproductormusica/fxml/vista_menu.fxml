<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="root" prefHeight="530.0" prefWidth="425.0" styleClass="fondo" stylesheets="@../css/MenuCSS1.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.apache.poi.reproductormusica.Vista_menuController">
   <children>
         <fx:include fx:id="vistaPrincipal" source="VistaPrincipal.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <HBox fx:id="menuHBox" AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox fx:id="menu" prefWidth="160.0" styleClass="barraMenu" stylesheets="@../css/MenuCSS1.css">
               <children>
                  <Button fx:id="botonAbrirCancion" alignment="TOP_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#abrirCancion" styleClass="botonesMenu" stylesheets="@../css/MenuCSS1.css" text="Abrir Cancion">
                     <VBox.margin>
                        <Insets bottom="1.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="botonAbrirCarpeta" alignment="TOP_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" styleClass="botonesMenu" text="Abrir Carpeta">
                     <VBox.margin>
                        <Insets bottom="1.0" left="5.0" right="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="botonOpciones" alignment="TOP_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" styleClass="botonesMenu" text="Opciones">
                     <VBox.margin>
                        <Insets bottom="1.0" left="5.0" right="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="botonSalir" alignment="TOP_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#exitButton" styleClass="botonesMenu" text="Salir">
                     <VBox.margin>
                        <Insets bottom="1.0" left="5.0" right="5.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <Button fx:id="botonMenu" mnemonicParsing="false" onAction="#mostrarMenu" prefHeight="52.0" prefWidth="0.0" styleClass="botonMenu" text="&gt;" />
         </children>
      </HBox>
   </children>
</AnchorPane>
