<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.apache.poi.reproductormusica.VistaPrincipalController">
   <children>
      <VBox alignment="CENTER" prefHeight="332.0" prefWidth="373.0">
         <children>
            <ImageView fx:id="imagenPortada" fitHeight="205.0" fitWidth="189.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </ImageView>
            <Label fx:id="labelTitulo">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="labelAutor">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin></Label>
            <Label fx:id="labelAbum" />
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="duracionActual" text="Label" />
            <Slider fx:id="sliderTiempo" prefHeight="14.0" prefWidth="226.0">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Slider>
            <Label fx:id="duracionTotal" text="Label" />
         </children>
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="botonAtras" mnemonicParsing="false" onAction="#pulsadoAtras" text="&lt;&lt;">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="30.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="botonPlay" mnemonicParsing="false" onAction="#pulsadorPlay" text="&gt;">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Button fx:id="botonAlante" mnemonicParsing="false" onAction="#pulsarAlante" text="&gt;&gt;">
               <HBox.margin>
                  <Insets bottom="10.0" left="30.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
   </padding>
</VBox>
